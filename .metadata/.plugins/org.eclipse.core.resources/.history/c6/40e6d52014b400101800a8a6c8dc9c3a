/**********************************************************************/
void setup(void)
{
	// CLOCK
	// AHB1 = RCC_AHB1ENR, GPIOA EN = bit 1
	// RCC = 0x40020000UL + 0x3800UL; RCC_AHB1ENR (RCC AHB1 peripheral clock enable register) = RCC + 0x30
	uint32_t *pAHBEnr = 	(uint32_t *)0x40020000UL + 0x3800UL + 0x30UL;
	// RCC_AHB1ENR Enable GPIOA (bit 1)
	*pAHBEnr|=(1<<0);

	// GPIO
	// mode = ouptup,  Output type = 0 Output push-pull, speed = 00: Low speed
	// GPIOx_MODER Address offset: 0x00.  Pin 0 = Bit [0,1] , Pin 1 = Bit [2,3] , Pin 3 = Bit [4,5]
	// GPIOA base address = AHB1_BASE =  0x40020000UL
	// GPIOx_OTYPER Address offset: 0x04

}


/**********************************************************************/
void delay(uint32_t time)
{
	for (uint32_t i =0 ; i< time; i++);
}
