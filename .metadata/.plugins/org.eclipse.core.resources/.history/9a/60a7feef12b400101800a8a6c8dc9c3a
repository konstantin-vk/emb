/**********************************************************************/
void setup(void)
{
	// CLOCK
	// AHB1
	// AHB1ENR Offset: 0x30; RCC AHB1 peripheral clock enable register
	// RCC = AHB1_BASE + 0x3800
	( RCC->AHB1ENR |= (1<<0) )
	// GPIO
	// mode = ouptup,  Output type = 0 Output push-pull, speed = 00: Low speed

}


/**********************************************************************/
void delay(uint32_t time)
{
	for (uint32_t i =0 ; i< time; i++);
}
